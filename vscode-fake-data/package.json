{
  "name": "vscode-fake-data",
  "displayName": "vscode-fake-data",
  "description": "Generate fake data for testing purpose.",
  "version": "1.0.0",
  "publisher": "cbouchard",
  "engines": {
    "vscode": "^1.84.0"
  },
  "repository": {
    "url": "https://github.com/cyrvan-b/vscode-fake-data"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsf.fake.uuid",
        "title": "Fake: Uuid"
      },
      {
        "command": "vsf.fake.fullname",
        "title": "Fake: Full Name"
      },
      {
        "command": "vsf.fake.paragraph",
        "title": "Fake: Paragraph"
      },
      {
        "command": "vsf.fake.words",
        "title": "Fake: Words"
      },
      {
        "command": "vsf.fake.avatar",
        "title": "Fake: Avatar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce package && vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
